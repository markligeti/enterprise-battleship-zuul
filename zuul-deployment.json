{
  "kind": "Deployment",
  "apiVersion": "apps/v1",
  "metadata": {
    "name": "zuul-gateway",
    "labels": {
      "k8s-app": "zuul-gateway",
      "service": "zuul"
    }
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "k8s-app": "zuul-gateway",
        "service": "zuul"
      }
    },
    "template": {
      "metadata": {
        "name": "zuul-gateway",
        "labels": {
          "k8s-app": "zuul-gateway",
          "service": "zuul"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "zuul-gateway",
            "image": "safranek/enterprise-battleship-zuul:latest"
          }
        ]
      }
    }
  }
}
